{
  "expo": {
    "name": "Thrilled",
    "slug": "thrilled",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "backgroundColor": "#121212",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.8",
    "orientation": "portrait",
    "primaryColor": "#3d5afe",
    "icon": "./assets/icon.png",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#121212"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#303f9f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.remypoirier.thrilled",
      "buildNumber": "1.0.8",
      "icon": "./assets/icon.png"
    },
    "android": {
      "package": "com.remypoirier.thrilled",
      "versionCode": 8,
      "permissions": []
    },
    "notification": {
      "icon": "./assets/icon_notification.png",
      "color": "#ffffff"
    },
    "description": "This is a very simple application displaying some stories to users. For now, all the application is in French",
    "githubUrl": "https://github.com/remy-poirier/stories-app"
  }
}
